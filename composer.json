{
  "name": "slavkluev/hexlet-psr-linter",
  "type": "library",
  "description": "PHP linter library",
  "license": "MIT",
  "keywords": ["PHP", "linter", "PSR"],
  "homepage": "https://github.com/slavkluev/hexlet-psr-linter",
  "authors": [
    {
      "name": "Viacheslav Kliuev",
      "email": "slavkluev@yandex.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "PSRLinter\\": "src/"
    }
  },
  "require": {
    "php": "^7.0",
    "nikic/php-parser": "2.1.0",
    "nategood/commando": "0.2.8",
    "league/climate": "*",
    "doctrine/inflector": "1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "squizlabs/php_codesniffer": "2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "bin": ["bin/psrlint"]
}